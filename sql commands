
//create table
CREATE TABLE `ajax_assign`.`base` ( `id` INT(11) NOT NULL AUTO_INCREMENT , `category` VARCHAR(255) NOT NULL , `sub_cat` VARCHAR(255) NOT NULL , `sub_sub` VARCHAR(255) NOT NULL , `created_at` TIMESTAMP NOT NULL DEFAULT CURRENT_TIMESTAMP , `updated_at` TIMESTAMP NOT NULL DEFAULT CURRENT_TIMESTAMP , PRIMARY KEY (`id`)) ENGINE = InnoDB;



